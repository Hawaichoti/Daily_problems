class Solution:
    def isMatch(self, s: str, p: str) -> bool:
        c=0
        if s == p:
            return True
        else:
            if len(s)==len(p) and s[0]==p[0]:
                return True
            if len(s)==len(p) and '*' in p or '.' in p:
                return True

            return False

        
